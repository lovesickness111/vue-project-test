{
  "name": "hello-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:unit-file-only": "vue-cli-service test:unit --testPathPattern=CallApiPostList.spec",
    "test:unit-suite-only": "vue-cli-service test:unit --testNamePattern=Test component ChildComponent",
    "test:debug": "node --inspect-brk ./node_modules/@vue/cli-service/bin/vue-cli-service.js test:unit --runInBand",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@progress/kendo-drawing": "^1.17.1",
    "@progress/kendo-licensing": "^1.2.2",
    "@progress/kendo-theme-default": "^5.8.1",
    "@progress/kendo-vue-buttons": "^3.6.3",
    "@progress/kendo-vue-dialogs": "^3.6.3",
    "@progress/kendo-vue-dropdowns": "^3.6.3",
    "@progress/kendo-vue-editor": "^3.6.3",
    "@progress/kendo-vue-inputs": "^3.6.3",
    "@progress/kendo-vue-intl": "^3.6.3",
    "@progress/kendo-vue-layout": "^3.6.3",
    "@progress/kendo-vue-pdf": "^3.6.3",
    "@progress/kendo-vue-popup": "^3.6.3",
    "@progress/kendo-vue-progressbars": "^3.6.3",
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "flush-promises": "^1.0.2",
    "vue": "^2.6.14",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "jest-serializer-vue": "^2.0.2",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
